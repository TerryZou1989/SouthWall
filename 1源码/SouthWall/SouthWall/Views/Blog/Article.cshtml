@{
    var entity = this.ViewData["entity"] as DatasEntity;
    ViewData["Title"] = entity.F_Title;
    ViewData["Description"] = entity.F_Content;
}
<iframe src='@(entity.F_Url)' scrolling='yes' border='0' frameborder='no' framespacing='0' allowfullscreen='true' class='w-100' style="height:calc(100% - 10px)"></iframe>

